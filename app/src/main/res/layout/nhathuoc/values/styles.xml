<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="ImageV1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:src">@drawable/back</item>
    </style>
    <style name="ImageV2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginLeft">18dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:src">@drawable/back</item>
    </style>
    <style name="ImageV3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">18dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:src">@drawable/plus</item>
    </style>
    <style name="ImageV4">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.5</item>
        <item name="android:src">@drawable/delete</item>
    </style>
    <style name="ImageV5">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.5</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:src">@drawable/edit</item>
    </style>
    <style name="Header1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/header_bg</item>
    </style>
    <style name="TxtView1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:fontFamily">serif</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">25dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TxtView2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TxtView3">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TxtView4">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textColor">#21211F</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TxtView5">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textColor">#21211F</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="View1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    <style name="LinearLayout1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:weightSum">10</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">#7FACF0</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="LinearLayout2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:weightSum">10</item>
        <item name="android:padding">10dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="ButtonCustom" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/button_default1</item>
    </style>
    <style name="ButtonCustom1" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/button_default2</item>
    </style>




</resources>